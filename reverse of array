#include<stdio.h>
int main()
{
 int n,i;
 printf("Enter number of integers\n");
 scanf("%d",&n);
 int arr[n];
 printf("Enter values of an array\n");
 for (i=0;i<n;i++)
 {

   scanf("%d",&arr[i]);
 }
 int rev[n];
 int j=0;
 printf("Enter reverse of an array:\n");
 for(i=n-1;i>=0;i--)
 {
   rev[j]=arr[i];
   printf("%d\t",rev[j]);
   j++;
  }
 } 
 
