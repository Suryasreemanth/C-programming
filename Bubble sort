#include<stdio.h>
int main()
{
int i,j,n;
printf("Enter number of elements:\n");
scanf("%d",&n);
int arr[n];
printf("Enter value of elements\n");
for (i=0;i<n;i++)
{
scanf("%d",&arr[i]);
}
for (i=0;i<n;i++)
{
   for(j=0;j<n-i-1;j++)
   {
     if(arr[j]>arr[j+1])
     {
       int temp=arr[j];
       arr[j]=arr[j+1];
       arr[j+1]=temp;
     }
    }
 }
 for i=0;i<n;i++)
 printf("%d\t",arr[i]);
